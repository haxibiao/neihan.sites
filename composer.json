{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"require": {
		"php": "^7.1.3",
		"Hxb/CategoryCount": "*",
		"Hxb/CategoryLikeCount": "*",
		"coderello/laravel-nova-lang": "^1.3",
		"doctrine/dbal": "^2.9",
		"fideloper/proxy": "^4.0",
		"freyo/flysystem-qcloud-cos-v4": "^2.1",
		"freyo/flysystem-qcloud-cos-v5": "1.1.7.1",
		"geoip2/geoip2": "~2.0",
		"halimtuhu/array-images": "^1.1",
		"haxibiao/graphql": "@dev",
		"inspheric/nova-url-field": "^1.1",
		"intervention/image": "^2.4",
		"jingling0101/laravel-qcloud-cos": "*",
		"laravel/framework": "5.8.*",
		"laravel/nova": "*",
		"laravel/passport": "^7.3",
		"laravel/tinker": "^1.0",
		"laravelcollective/html": "^5.7",
		"lokielse/omnipay-alipay": "^3.0",
		"mll-lab/laravel-graphql-playground": "^1.0",
		"nuwave/lighthouse": "3.7.0",
		"overtrue/easy-sms": "^1.1",
		"overtrue/laravel-pinyin": "~3.0",
		"piwik/piwik-php-tracker": "^1.4",
		"predis/predis": "^1.1",
		"pusher/pusher-php-server": "^3.4",
		"qcloud/vod-sdk-v5": "^2.2",
		"torann/geoip": "^1.0"
	},
	"require-dev": {
		"beyondcode/laravel-dump-server": "^1.0",
		"filp/whoops": "^2.0",
		"fzaninotto/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^2.0",
		"phpunit/phpunit": "^7.0"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"secure-http": false
	},
	"repositories": {
		"0": {
			"type": "vcs",
			"url": "http://code.haxibiao.cn/web/graphql.git"
		},
		"1": {
			"type": "vcs",
			"url": "http://code.haxibiao.cn/web/nova2.git"
		},
		"packagist": {
			"type": "composer",
			"url": "https://mirrors.aliyun.com/composer/"
		},
		"2": {
			"type": "path",
			"url": "./nova-components/HotCatoryCount"
		},
		"3": {
			"type": "path",
			"url": "./nova-components/CategoryCount"
		},
		"4": {
			"type": "path",
			"url": "./nova-components/CategoryLikeCount"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"database/seeds",
			"database/factories"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	}
}
