{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2.5",
    "Hxb/CategoryCount": "*",
    "Hxb/CategoryLikeCount": "*",
    "acme/price-tracker": "*",
    "anerg2046/sns_auth": "^2.0",
    "coderello/laravel-nova-lang": "^1.4",
    "doctrine/dbal": "^2.10",
    "fideloper/proxy": "^4.3",
    "freyo/flysystem-qcloud-cos-v5": "^2.0",
    "fruitcake/laravel-cors": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "halimtuhu/array-images": "^1.1",
    "haxibiao/pay": "2.9.3",
    "inspheric/nova-url-field": "^1.4",
    "intervention/image": "^2.5",
    "jenssegers/agent": "^2.6",
    "jingling0101/laravel-qcloud-cos": "^4.0",
    "laravel/framework": "^7.0",
    "laravel/nova": "^3.0@dev",
    "laravel/passport": "^8.4",
    "laravel/tinker": "^2.0",
    "laravelcollective/html": "^6.1",
    "lokielse/omnipay-alipay": "^3.0",
    "mll-lab/laravel-graphql-playground": "^2.1",
    "nuwave/lighthouse": "^4.10",
    "overtrue/laravel-pinyin": "^4.0",
    "php-ffmpeg/php-ffmpeg": "^0.14.0",
    "piwik/piwik-php-tracker": "^2.0",
    "predis/predis": "^1.1",
    "qcloud/cos-sdk-v5": "^2.0",
    "simplesoftwareio/simple-qrcode": "^2.0",
    "spatie/laravel-backup": "^6.5.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.4",
    "facade/ignition": "^2.0",
    "filp/whoops": "^2.7",
    "fzaninotto/faker": "^1.9.1",
    "laravel/helpers": "^1.2",
    "laravel/ui": "^2.0",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "overtrue/easy-sms": "^1.1",
    "tencentcloud/tencentcloud-sdk-php": "3.0.94",
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "secure-http": false
  },
  "repositories": {
    "1": {
      "type": "vcs",
      "url": "http://code.haxibiao.cn/web/nova.git"
    },
    "3": {
      "type": "path",
      "url": "./nova-components/CategoryCount"
    },
    "4": {
      "type": "path",
      "url": "./nova-components/CategoryLikeCount"
    },
    "5": {
      "type": "path",
      "url": "./nova-components/PriceTracker"
    },
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Haxibiao\\Config\\": "packages/haxibiao/config/src/",
      "Haxibiao\\Helpers\\": "packages/haxibiao/helpers/src/",
      "Haxibiao\\Task\\": "packages/haxibiao/task/src/",
      "Haxibiao\\Base\\": "packages/haxibiao/user/src/",
      "Haxibiao\\Matomo\\": "packages/haxibiao/matomo/src/",
      "Haxibiao\\Store\\": "packages/haxibiao/store/src/",
      "Haxibiao\\Content\\": "packages/haxibiao/content/src/",
      "Haxibiao\\Media\\": "packages/haxibiao/media/src/",
      "Haxibiao\\Question\\": "packages/haxibiao/question/src/",
      "Haxibiao\\Live\\": "packages/haxibiao/live/src/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}