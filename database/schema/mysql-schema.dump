  `type` tinyint NOT NULL DEFAULT '0' COMMENT '类型：默认RMB钱包',
  `pay_account` varchar(60) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '提现账号',
